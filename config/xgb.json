{
    "data": {
        "name": "diamonds",
        "source": {
            "getLocal": true,
            "url": "https://raw.githubusercontent.com/xtreamsrl/xtream-ai-assignment-engineer/main/datasets/diamonds/diamonds.csv",
            "localPath": "diamonds.csv"
        },
        "cleaning": {
            "dropColumns": {
                "enabled": true,
                "columns": ["Unnamed: 0", "test"]
            },
            "dropDuplicates": true,
            "dropNa": true,
            "dropCustom": {
                "enabled": true,
                "columns": {
                    "price": {
                        "min": 0,
                        "max": 100000000000,
                        "rangeOrEqual": true
                    },
                    "x": {
                        "value": 0,
                        "rangeOrEqual": false
                    },
                    "y": {
                        "value": 0,
                        "rangeOrEqual": false
                    },
                    "z": {
                        "value": 0,
                        "rangeOrEqual": false
                    }
                }
            }
        },
        "exploration": {
            "figsize": {
                "width": 14,
                "height": 10
            },
            "scatter_matrix": {
                "enabled": false
            },
            "hist": {
                "enabled": false,
                "bins": 100
            },
            "categorical": {
                "enabled": false,
                "columns": ["cut", "color", "clarity"]
            }
        },
        "processing": {
            "dropColumnsPostExploration": {
                "enabled": false,
                "columns": ["depth", "table", "y", "z"]
            },
            "getDummies": {
                "enabled": false,
                "columns": ["cut", "color", "clarity"]
            },
            "orderCategorical": {
                "enabled": true,
                "columns": {
                    "cut": ["Fair", "Good", "Very Good", "Ideal", "Premium"],
                    "color": ["D", "E", "F", "G", "H", "I", "J"],
                    "clarity": ["IF", "VVS1", "VVS2", "VS1", "VS2", "SI1", "SI2", "I1"]
                }
            },
            "trainTestSplit": {
                "target": ["price"],
                "testSize": 0.2,
                "randomState": 42
            }
        }
    },
    "model": {
        "enabled": true,
        "type": "xgb_regression",
        "parameters": {
            "enable_categorical": true,
            "random_state": 42
        },
        "evaluation": {
            "enabled": true,
            "metrics": ["r2_score", "mean_absolute_error"]
        },
        "save": {
            "enabled": true,
            "filename": "model.pkl"
        },
        "exploration": {
            "gof": {
                "enabled": true
            }
        },
        "transformation": {
            "enabled": true,
            "func": ["log"]
        },
        "optuna_tuning": {
            "enabled": false,
            "metric": "mean_absolute_error",
            "nTrials": 100,
            "direction": "minimize",
            "hyperparameters": {
                "n_estimators": {
                    "cat": [100, 1000],
                    "log": false
                },
                "lambda": {
                    "range": [1e-8, 1.0],
                    "log": true
                },
                "alpha": {
                    "range": [1e-8, 1.0],
                    "log": true
                },
                "colsample_bytree": {
                    "cat": [0.3, 0.4, 0.5, 0.7],
                    "log": false
                },
                "subsample": {
                    "cat": [0.5, 0.6, 0.7, 0.8, 0.9, 1.0],
                    "log": false
                },
                "learning_rate": {
                    "range": [1e-8, 1.0],
                    "log": true
                },
                "max_depth": {
                    "range": [3, 9],
                    "log": false
                },
                "min_child_weight": {
                    "range": [1, 10],
                    "log": false
                }
            }
        }
    },
    "deploy": {
        "enabled": true,
        "model_name": {
            "trainOnTheSpot": true,
            "sameAsTraining": true,
            "epoch": "1721058558"
        }
    }
}