{
    "data": {
        "name": "diamonds",
        "source": {
            "getLocal": true,
            "url": "https://raw.githubusercontent.com/xtreamsrl/xtream-ai-assignment-engineer/main/datasets/diamonds/diamonds.csv",
            "localPath": "diamonds.csv"
        },
        "cleaning": {
            "dropColumns": {
                "enabled": true,
                "columns": ["Unnamed: 0", "test"]
            },
            "dropDuplicates": true,
            "dropNa": true,
            "dropCustom": {
                "enabled": true,
                "columns": {
                    "price": {
                        "min": 0,
                        "max": 100000000000,
                        "rangeOrEqual": true
                    },
                    "x": {
                        "value": 0,
                        "rangeOrEqual": false
                    },
                    "y": {
                        "value": 0,
                        "rangeOrEqual": false
                    },
                    "z": {
                        "value": 0,
                        "rangeOrEqual": false
                    }
                }
            }
        },
        "exploration": {
            "figsize": {
                "width": 14,
                "height": 10
            },
            "scatter_matrix": {
                "enabled": true
            },
            "hist": {
                "enabled": true,
                "bins": 100
            },
            "categorical": {
                "enabled": true,
                "columns": ["cut", "color", "clarity"]
            }
        },
        "processing": {
            "dropColumnsPostExploration": {
                "enabled": true,
                "columns": ["depth", "table", "y", "z"]
            },
            "getDummies": {
                "enabled": true,
                "columns": ["cut", "color", "clarity"]
            },
            "trainTestSplit": {
                "target": ["price"],
                "testSize": 0.2,
                "randomState": 42
            }
        }
    },
    "model": {
        "enabled": false,
        "type": "linear_regression",
        "hyperparameters": {
            "fit_intercept": true,
            "normalize": false,
            "copy_X": true,
            "n_jobs": null
        }
    }
}